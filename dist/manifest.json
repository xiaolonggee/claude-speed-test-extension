{
  "manifest_version": 3,
  "name": "Claude API 测速工具",
  "description": "测试 Claude API 第三方接口的首字时间和性能",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    },
    "default_title": "Claude API 测速"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}

